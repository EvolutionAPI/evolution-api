  updatedAt DateTime  @updatedAt @map("updated_at") @db.Timestamptz()

  instanceId String   @unique @map("instance_id")
  Instance   Instance @relation(fields: [instanceId], references: [id], onDelete: Cascade)

  @@map("proxies")
}

model Setting {
